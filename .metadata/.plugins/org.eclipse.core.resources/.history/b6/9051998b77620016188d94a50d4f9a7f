package daniel.game;

import java.util.Scanner;

/**
 * @author Daniel
 *
 */
public class Game {
	private Player player;
	private Scanner playerInput;
	
	public Game(){
		playerInput = new Scanner(System.in);
		player = new Player();
	}
	public void run(){
		System.out.println(player.getMoney());
	}
	
	

}
